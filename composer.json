{
    "name": "symfony/symfony-standard",
    "license": "MIT",
    "type": "project",
    "description": "Wealthbot - financial Robo Advisor system",
    "autoload": {
        "psr-4": {
            "Wealthbot\\": "src/Wealthbot"
        },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "autoload-dev": {
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php",
            "app/polyfill/apc.php"
        ]
    },
    "require": {
        "php": ">=7.3.1",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "jms/security-extra-bundle": "1.6.1",
        "sensio/distribution-bundle": "^5.0.19",
        "sensio/framework-extra-bundle": "^5.0.0",
        "symfony/assetic-bundle": "2.*",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/swiftmailer-bundle": "^2.6.4",
        "symfony/symfony": "3.4.*",
        "twig/extensions": "1.5.4",
        "twig/twig": "^1.0||^2.0",
        "friendsofsymfony/user-bundle": "2.1.2",
        "hpatoio/deploy-bundle": "1.5.*",
        "doctrine/data-fixtures": "1.3.*",
        "doctrine/doctrine-fixtures-bundle": "3.1.*",
        "stof/doctrine-extensions-bundle": "1.3.*",
        "knplabs/knp-paginator-bundle": "2.8.*",
        "knplabs/knp-snappy-bundle": "1.6.*",
        "doctrine/mongodb": "1.6.*",
        "doctrine/mongodb-odm": "1.2.*",
        "doctrine/mongodb-odm-bundle": "3.5.*",
        "jms/serializer-bundle": "2.4.3",
        "friendsofsymfony/jsrouting-bundle": "2.2.*",
        "nelmio/api-doc-bundle": "3.3.*",
        "phpoffice/phpspreadsheet": "1.6.*"

    },
    "require-dev": {
        "sensio/generator-bundle": "3.1.*",
        "symfony/phpunit-bridge": "3.4.*"
    },
    "scripts": {
        "symfony-scripts": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "platform": {
            "php": "7.3.1"
        }
    },

    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "branch-alias": {
            "dev-master": "3.4.x-dev"
        }
    }
}
