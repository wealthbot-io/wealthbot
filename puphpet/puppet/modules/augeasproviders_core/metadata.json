      "operatingsystemrelease": [
        "12.04",
        "12.10",
        "14.04",
        "14.10",
        "15.10",
        "16.04"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "4",
        "5",
        "6",
        "7"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 2.7.0 < 6.0.0"
    }
  ]
}
